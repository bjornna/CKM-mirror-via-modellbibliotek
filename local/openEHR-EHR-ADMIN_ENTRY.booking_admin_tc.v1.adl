				["at0003"] = <
					text = <"Aviseringsbrev">
					description = <"">
				>
				["at0004"] = <
					text = <"Kopplad undersökning">
					description = <"Ett visningsnamn för undersökningen kopplad till denna bokning. Exempel: \"EKG\"">
				>
				["at0005"] = <
					text = <"Självincheckning tillåten?">
					description = <"\"True\" om patienten får självinchecka (t.ex. vid en terminal) för denna bokning. Om servern inte har fastställt om självincheckning är tillåten, är detta attribut null. Självincheckning är tillåten om tidstypen tillåter självincheckning, att det finns en associerad betalningsenhet till bokningen osv. Detta möjliggör då att en avgift kan beräknas automatiskt, utan användarens inblandning.">
				>
				["at0006"] = <
					text = <"Självincheckningskod">
					description = <"Fyrsiffrig kod som patienten kan använda vid självincheckning vid terminalen. Detta kan vara null för äldre bokningar.">
				>
				["at0009"] = <
					text = <"Patient kommentar">
					description = <"Om bokningen/ombokning/avbokning gjordes av patienten från en patientportal, är detta kommentaren som patienten gav. Detta visas per bokningstransaktion och sammanfattas en gång i hela bokningsöversikten.">
				>
				["at0012"] = <
					text = <"Studie">
					description = <"Textkommentar tillagd till bokningen för att informera om besöket är en del av någon studie eller forskning.">
				>
				["at0013"] = <
					text = <"Långremissdatum">
					description = <"Giltighetsdatum för remissen som ledde till denna bokning. Vissa är giltiga över flera år och kallas långremissdatum">
				>
				["at0014"] = <
					text = <"Besökstyp">
					description = <"Typ av besök, t ex återbesök eller nybesök">
				>
				["at0015"] = <
					text = <"Datum patienten informerad">
					description = <"">
				>
				["at0016"] = <
					text = <"Mall SMS-avisering">
					description = <"">
				>
				["at0017"] = <
					text = <"Är patientens första besök?">
					description = <"Sant om denna bokning är ett nytt besök enligt vårdgarantin.">
				>
				["at0018"] = <
					text = <"Är en specialistremiss">
					description = <"Sant om patienten har en remiss från en specialist för denna bokning.">
				>
				["at0019"] = <
					text = <"Specialistremissen till datum">
					description = <"Specialistremissens \"giltig till-datum\" för denna bokning.">
				>
				["at0020"] = <
					text = <"Behövs tolk?">
					description = <"Sant om patienten behöver en tolk för att underlätta kommunikationen under besöket.">
				>
				["at0021"] = <
					text = <"Patienten tillgänglig med kort varsel?">
					description = <"Sant om patienten kan komma med kort varsel; då kan de kontaktas om en tid blir tillgänglig med kort varsel.">
				>
				["at0022"] = <
					text = <"Receptions kommentar">
					description = <"Kommentarer som måste visas i receptionen angående denna bokning.">
				>
				["at0023"] = <
					text = <"Beslutsdatum åtgärd">
					description = <"Datum då det beslutades att patienten behövde behandlingen eller ingreppet relaterat till denna bokning.">
				>
				["at0024"] = <
					text = <"Ankomstdatum remiss">
					description = <"">
				>
				["at0025"] = <
					text = <"Patientens länskod">
					description = <"Länet där patienten är folkbokförd">
				>
				["at0026"] = <
					text = <"Remissens ursprung">
					description = <"Källa från vilken bokningen gjordes (webb, applikation etc.)">
				>
				["at0027"] = <
					text = <"Ombud">
					description = <"Ursprunglig bokning gjordes av ett ombud som agerade på patientens begäran.">
				>
				["at0029"] = <
					text = <"Tidsprioritering">
					description = <"Tidprioritering för väntelisteposten, t.ex. 6M vilket översätts till: idag + 6 månader när en ny bokning skapas från väntelistan. Exempel: \"6M\"">
				>
				["at0030"] = <
					text = <"Önskat besöksdatum för patient">
					description = <"Om patienten är eller har varit på en väntelista, är detta datumet då personalen önskar boka patienten. Exempel: 20200506">
				>
				["at0031"] = <
					text = <"SMS senast ändrad information">
					description = <"Information om när SMS-mallen ändrades i bokningen. Detta kan vara null om ingen SMS-mall har valts eller ändrats.">
				>
				["at0034"] = <
					text = <"Patientavisering">
					description = <"">
				>
				["at0039"] = <
					text = <"Undersökning">
					description = <"">
				>
				["at0040"] = <
