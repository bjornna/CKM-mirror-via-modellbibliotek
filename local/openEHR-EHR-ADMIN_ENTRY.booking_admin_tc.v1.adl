definition
	ADMIN_ENTRY[at0000] matches {    -- Booking admin
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0041] occurrences matches {0..1} matches {    -- Healthcare plan ID
						value matches {
							DV_IDENTIFIER matches {*}
						}
					}
					CLUSTER[at0039] occurrences matches {0..*} matches {    -- Examination
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0040] occurrences matches {0..1} matches {    -- Examination date time
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0004] occurrences matches {0..1} matches {    -- Linked examination
								value matches {
									DV_CODED_TEXT matches {*}
								}
							}
						}
					}
					CLUSTER[at0034] occurrences matches {0..1} matches {    -- Patient notification
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0015] occurrences matches {0..1} matches {    -- Date patient notified
								value matches {
									DV_DATE matches {*}
								}
							}
							ELEMENT[at0002] occurrences matches {0..1} matches {    -- Notification type
								value matches {
									DV_CODED_TEXT matches {*}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0003] occurrences matches {0..1} matches {    -- Notification letter
								value matches {
