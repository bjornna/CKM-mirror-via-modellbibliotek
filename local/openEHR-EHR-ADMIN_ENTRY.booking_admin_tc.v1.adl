						}
					}
					ELEMENT[at0027] occurrences matches {0..1} matches {    -- Agent
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0029] occurrences matches {0..1} matches {    -- Time priority
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {    -- Long referral arrival date
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0017] occurrences matches {0..1} matches {    -- Is patients first visit?
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					ELEMENT[at0018] occurrences matches {0..1} matches {    -- Is a specialist referral
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					ELEMENT[at0026] occurrences matches {0..1} matches {    -- Origin of referral
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0019] occurrences matches {0..1} matches {    -- Specialist referral through date
						value matches {
