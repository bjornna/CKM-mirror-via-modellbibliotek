ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Booking admin">
					description = <"Booking admin">
				>
				["at0001"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Notification type">
					description = <"">
				>
				["at0003"] = <
					text = <"Notification letter">
					description = <"">
				>
				["at0004"] = <
					text = <"Linked examination">
					description = <"A display name for the examination linked to this booking Example : \"ECG\"">
				>
				["at0005"] = <
					text = <"Self check in allowed?">
					description = <"True if the patient is allowed to self check in (e.g. at a terminal) for this booking. If the server hasn’t computed whether self check in is allowed, this attribute is null. Self check in is allowed if the time type allows self check in, there is an associated billing unit to the booking etc. This then allows a charge to be computed automatically, without user intervention.">
				>
				["at0006"] = <
					text = <"Self check in code">
					description = <"4 digit code that the patient can use when performing self check in at the terminal. This can be null for older bookings.">
				>
				["at0009"] = <
					text = <"Patient comment">
					description = <"If the booking was made/rebooked/cancelled by the patient from a patient portal, this is the comment given by patient. This is shown per booking transaction and summarized once into whole booking summary. ">
				>
				["at0012"] = <
					text = <"Study">
					description = <"Text comment added to the booking to inform whether the visit is a part of any study or research.">
				>
				["at0013"] = <
					text = <"Long referral arrival date">
					description = <"The valid until date of referral that led to this booking. Some are valid over several years and called as longReferralArrivalDate (sv: 'långremissdatum').">
				>
				["at0014"] = <
					text = <"Visit type">
					description = <"The type of visit, e.g. a return visit vs a first visit.">
				>
				["at0015"] = <
					text = <"Date patient notified">
					description = <"">
				>
				["at0016"] = <
					text = <"SMS notification template">
					description = <"">
				>
				["at0017"] = <
					text = <"Is patients first visit?">
					description = <"True if this booking is a new visit according to care guarantee.">
				>
				["at0018"] = <
					text = <"Is a specialist referral">
					description = <"True if patient has a referral from a specialist for this booking. ">
				>
				["at0019"] = <
					text = <"Specialist referral through date">
					description = <"Specialist referral \"valid until date\" for this booking.">
				>
				["at0020"] = <
					text = <"Is interpreter required?">
					description = <"True if patient requires an interpreter to facilitate communication during the visit.">
				>
				["at0021"] = <
					text = <"Patient available on short notice?">
					description = <"True if the patient can come at short notice, then they can be called if a time becomes available at short notice.">
				>
				["at0022"] = <
					text = <"Counter comment">
					description = <"Comments that must be shown in counter regarding this booking.">
				>
				["at0023"] = <
					text = <"Task decision date">
					description = <"Date when it was decided the patient needed the treatment or procedure related to this booking.">
				>
				["at0024"] = <
					text = <"Referral arrival date">
					description = <"">
				>
				["at0025"] = <
					text = <"Patient residence county code">
					description = <"The county code in which booked patient resides. ">
				>
				["at0026"] = <
					text = <"Origin of referral">
					description = <"Origin from which booking was made (web, application etc)">
				>
				["at0027"] = <
					text = <"Agent">
					description = <"Original booking was made by agent (sv: 'ombud') acting on patient’s request.">
				>
				["at0029"] = <
					text = <"Time priority">
					description = <"Time priority for the waiting list entry. e.g. 6M which is translated to: today+6 month when creating a new booking from the waiting list. Example : \"6M\"">
				>
				["at0030"] = <
					text = <"Patient requested appointment date">
					description = <"If the patient is or has been on a waiting list, this is the date when staff wishes to book the patient. Example : 20200506">
				>
				["at0031"] = <
					text = <"SMS last change information">
					description = <"Information about when the SMS template was changed in the booking. This can be null if SMS template hasn’t been chosen/changed.">
				>
				["at0034"] = <
					text = <"Patient notification">
					description = <"">
				>
				["at0039"] = <
					text = <"Examination">
					description = <"">
				>
				["at0040"] = <
					text = <"Examination date time">
					description = <"">
				>
				["at0041"] = <
					text = <"Healthcare plan ID">
					description = <"">
				>
				["at0042"] = <
					text = <"Is fee free?">
					description = <"">
				>
			>
		>
		["sv"] = <
			items = <
				["at0000"] = <
					text = <"Bokning administration">
