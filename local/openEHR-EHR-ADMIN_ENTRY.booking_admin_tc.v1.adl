									DV_TEXT matches {*}
								}
							}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {    -- Self check in allowed?
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {    -- Self check in code
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {    -- Patient comment
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0012] occurrences matches {0..1} matches {    -- Study
