								defining_code matches {
									[local::
									at0015,    -- Yes
									at0016]    -- No
								}
							}
						}
					}
					ELEMENT[at0010] occurrences matches {0..1} matches {    -- Psychological support
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0017,    -- Yes
									at0018]    -- No
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0002] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- Contact date
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {    -- Assessment ID
						value matches {
							DV_IDENTIFIER matches {*}
						}
					}
				}
			}
		}
	}

