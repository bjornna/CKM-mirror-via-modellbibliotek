definition
	EVALUATION[at0000] matches {    -- SCR Palliative care assessment
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0007] occurrences matches {0..1} matches {    -- Pain control
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0011,    -- Yes
									at0012]    -- No
								}
							}
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {    -- Other symptom management
