definition
	CLUSTER[at0000] matches {    -- Breast_cancer_operation
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Type of intervention
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0002,    -- Performed oncoplasty
							at0003,    -- Clips in wound cavity
							at0004,    -- Contralateral intervention
							at0005,    -- Drainage
							at0006]    -- Oncoplasty
						}
					}
				}
			}
			allow_archetype CLUSTER[at0007] occurrences matches {0..*} matches {    -- CLUSTER_SLOT
				include
					archetype_id/value matches {/.*/}
			}
		}
	}

