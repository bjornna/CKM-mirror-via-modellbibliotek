definition
	EVALUATION[at0000] matches {    -- Treatment
		data matches {
			ITEM_TREE[at0003] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0053] occurrences matches {0..1} matches {    -- Treatment modality
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0054,    -- Medical oncology treatment
									at0055,    -- Radiotherapy
									at0056,    -- Surgery
									at0057,    -- Active surveillance
									at0058]    -- No cancer treatment
								}
							}
						}
					}
					ELEMENT[at0059] occurrences matches {0..1} matches {    -- Reason for no cancer treatment
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0060,    -- Lågt funktionsstatus enligt ECOG
									at0061,    -- Betydande samsjuklighet
									at0062,    -- Cancer i framskridande stadium
									at0063,    -- Död innan behandling
									at0064]    -- Annan orsak
								}
							}
						}
					}
					ELEMENT[at0065] occurrences matches {0..1} matches {    -- Other reason
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0052] matches {    -- Treatment intent
						value matches {
							DV_CODED_TEXT matches {*}
						}
					}
					CLUSTER[at0038] occurrences matches {0..*} matches {    -- Treatment modality
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0005] occurrences matches {0..1} matches {    -- Category
								value matches {
									DV_CODED_TEXT matches {*}
								}
							}
							ELEMENT[at0066] occurrences matches {0..1} matches {    -- Other catagory
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0012] occurrences matches {0..1} matches {    -- Treatment details
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0029] occurrences matches {0..*} matches {    -- Details
								include
									archetype_id/value matches {/.*/}
							}
							ELEMENT[at0048] occurrences matches {0..1} matches {    -- Adjuvancy
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0049,    -- Neoadjuvant
											at0050,    -- Perioperative
											at0051]    -- Adjuvant
										}
									}
								}
							}
							ELEMENT[at0067] occurrences matches {0..1} matches {    -- Type of chemoradiotherapy
								value matches {
									DV_CODED_TEXT matches {*}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0046] occurrences matches {0..1} matches {    -- Date last updated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0047] occurrences matches {0..1} matches {    -- Plan identifier
						value matches {
							DV_IDENTIFIER matches {*}
						}
					}
					allow_archetype CLUSTER[at0002] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

