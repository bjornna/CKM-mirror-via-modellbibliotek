definition
	EVALUATION[at0000] matches {    -- behandlingsstrategi
		data matches {
			ITEM_TREE[at0003] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0037] occurrences matches {0..1} matches {    -- Synopsis
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0053] occurrences matches {0..1} matches {    -- Behandlingsåtgärd
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0054,    -- Medicinsk onkologisk behandling
									at0055,    -- Radioterapi
									at0056,    -- Kirurgi
									at0057,    -- Aktiv monitorering
									at0058]    -- Ingen tumörinriktad behandling
								}
							}
						}
					}
					ELEMENT[at0059] occurrences matches {0..1} matches {    -- Orsak ingen tumörinriktad behandling
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0060,    -- Lågt funktionsstatus enligt ECOG
									at0061,    -- Betydande samsjuklighet
									at0062,    -- Cancer i framskridande stadium
									at0063,    -- Död innan behandling
									at0064]    -- Annan orsak
								}
							}
						}
					}
					ELEMENT[at0065] occurrences matches {0..1} matches {    -- Specificera annan orsak
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0052] matches {    -- Behandlingsintention
						value matches {
							DV_CODED_TEXT matches {*}
						}
					}
					CLUSTER[at0038] occurrences matches {0..*} matches {    -- Behandlingsmodalitet
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0005] occurrences matches {0..1} matches {    -- Behandlingskategori
								value matches {
									DV_CODED_TEXT matches {*}
								}
							}
							ELEMENT[at0066] occurrences matches {0..1} matches {    -- Annan kategori
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0012] occurrences matches {0..1} matches {    -- Specificering av behandling
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0029] occurrences matches {0..*} matches {    -- Details
								include
									archetype_id/value matches {/.*/}
							}
							ELEMENT[at0014] occurrences matches {0..1} matches {    -- Line of treatment
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0015,    -- First
											at0016,    -- Second
											at0017]    -- Third
										}
									}
								}
							}
							ELEMENT[at0028] occurrences matches {0..1} matches {    -- Intended end date
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0048] occurrences matches {0..1} matches {    -- Adjuvancy
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0049,    -- Neoadjuvant
											at0050,    -- Perioperative
											at0051]    -- Adjuvant
										}
									}
								}
							}
							ELEMENT[at0027] occurrences matches {0..1} matches {    -- Intended start date
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0046] occurrences matches {0..1} matches {    -- Date last updated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0047] occurrences matches {0..1} matches {    -- Plan identifier
						value matches {
							DV_IDENTIFIER matches {*}
						}
					}
					allow_archetype CLUSTER[at0002] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

