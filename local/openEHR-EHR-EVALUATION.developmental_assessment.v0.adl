definition
	EVALUATION[at0000] matches {    -- Utveckling bedömning
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Typ av utveckling testning
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0003,    -- Psykologisk testning
									at0004]    -- Motorisk/neurologisk testning
								}
							}
						}
					}
					allow_archetype CLUSTER[at0007] occurrences matches {0..*} matches {    -- Psykologisk testning
						include
							archetype_id/value matches {/.*/}
					}
					allow_archetype CLUSTER[at0008] occurrences matches {0..*} matches {    -- Motorisk/neurologisk testning
						include
							archetype_id/value matches {/.*/}
					}
					allow_archetype CLUSTER[at0010] occurrences matches {0..*} matches {    -- Annan utvecklingsbedömning
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

