definition
	CLUSTER[at0000.1.1] occurrences matches {0..*} matches {    -- Followup question with CTCAE-tingling_numbness
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0002.1.1] occurrences matches {1..*} matches {    -- How do you experience your problems of tingling/numbness in your hands and/or feet?
				value matches {
					1|[local::at0.0.1],
					2|[local::at0.0.2],
					3|[local::at0.0.3],
					4|[local::at0.0.4]
				}
			}
			allow_archetype CLUSTER[at0004] occurrences matches {0..*} matches {    -- Additional followup details
				include
					archetype_id/value matches {/.*/}
			}
			ELEMENT[at0.1] occurrences matches {0..*} matches {    -- Related CTCAE term
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[ac0.1]
						}
					}
				}
			}
		}
	}

