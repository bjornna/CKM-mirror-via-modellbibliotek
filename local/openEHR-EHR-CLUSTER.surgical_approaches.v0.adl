definition
	CLUSTER[at0000] occurrences matches {0..*} matches {    -- Surgical approach
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..*} matches {    -- Arthroscopic approach
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0003,    -- Posterior portal
							at0004,    -- Anterior portal
							at0005,    -- Lateral portal
							at0006,    -- Anteroinferior portal
							at0007,    -- Posteriorinferior portal
							at0008,    -- Nevasier (supraspinatus) portal
							at0009,    -- Port of Wilmington (Posterolateral) portal
							at0016,    -- Anterolateral portal
							at0017,    -- Modified mid anterior portal
							at0018,    -- Distal anterolateral portal
							at0019,    -- Posterolateral portal
							at0020]    -- Dienst portal
						}
					}
				}
			}
			ELEMENT[at0002] occurrences matches {0..*} matches {    -- Open approach
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0010,    -- Deltopectoral
							at0011,    -- Anterolateral
							at0012,    -- Superior strap
							at0013,    -- Transacromial
							at0014,    -- Posterior
							at0021,    -- Anterior
							at0022,    -- Lateral
							at0023]    -- Medial
						}
					}
				}
			}
			ELEMENT[at0024] occurrences matches {0..1} matches {    -- Minimally Invasive approach
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::at0025]    -- Minimally Invasive approach
						}
					}
				}
			}
			ELEMENT[at0015] occurrences matches {0..1} matches {    -- Comment
				value matches {
					DV_TEXT matches {*}
				}
			}
		}
	}

