definition
	CLUSTER[at0000.1] matches {    -- Followup question
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0002.1] occurrences matches {1..*} matches {    -- Question
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.1,    -- Yes
							at0.2,    -- No
							at0.3]    -- Unknown
						}
					}
				}
			}
			allow_archetype CLUSTER[at0004] occurrences matches {0..*} matches {    -- Additional followup details
				include
					archetype_id/value matches {/.*/}
			}
		}
	}

