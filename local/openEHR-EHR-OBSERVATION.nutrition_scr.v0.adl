									}
									ELEMENT[at0007] occurrences matches {0..1} matches {    -- Willingness to eat
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0009,    -- Ja
													at0010]    -- Nej
												}
											}
										}
									}
									ELEMENT[at0011] occurrences matches {0..1} matches {    -- Helping aid
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0013,    -- Ja
													at0014]    -- Nej
												}
											}
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {    -- Type of helping aid
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0015,    -- Matning via perkutant endoskopisk gastrostomi (PEG
													at0016]    -- Sondmatning via nasal sond
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

